{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "nodemon Server",
      "runtimeExecutable": "nodemon",
      "program": "${workspaceFolder}/gRpc/server.js",
      "restart": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "DEBUG": "subber:*"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Server",
      "program": "${workspaceFolder}/gRpc/server.js",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "DEBUG": "subber:*"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cli",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/cli.js",
      "args": [
        "air",
        "info"
      ],
      "env": {
        "DEBUG": "subber:*"
      }
    },
  ]
}